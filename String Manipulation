import java.util.*;
public class StringManipulation{
    public static void main(String args[]){

        Scanner s = new Scanner(System.in);
        String A = s.nextLine();
        String B = s.nextLine();

        int result = A.compareTo(B);
        if(result < 0)
        System.out.println("No");
        
        String An = A.substring(0,1).toUpperCase()+A.substring(1);
        String Bn = B.substring(0,1).toUpperCase()+B.substring(1);

        System.out.println(An + " " + Bn);
        s.close();
        }
}
